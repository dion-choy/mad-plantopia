<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="fab">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
        <item name="cornerSizeTopLeft">100dp</item>
        <item name="cornerSizeTopRight">100dp</item>
        <item name="cornerSizeBottomLeft">100dp</item>
        <item name="cornerSizeBottomRight">100dp</item>
        <item name="colorPrimaryContainer">@color/nav_bar_dark_background</item>
    </style>

    <!-- Base application theme. -->
    <style name="Base.Theme.MADproj" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <item name="colorPrimary">@color/dark_primary</item>
        <item name="android:textColorSecondary">@color/nav_bar_dark_background</item>
        <item name="android:windowBackground">@color/dark_background</item>

        <item name="colorOnSecondaryContainer">@color/dark_text_em</item>
        <item name="colorOnSurface">@color/dark_text_em</item>

        <item name="colorOnSurfaceVariant">@color/dark_text</item>

        <item name="colorSecondaryContainer">@color/nav_bar_focus_dark</item>
        <item name="colorSurfaceContainer">@color/nav_bar_dark_background</item>

        <item name="colorPrimaryContainer">@color/nav_bar_dark_background</item>
        <item name="colorOnPrimaryContainer">@color/dark_text</item>
    </style>
</resources>